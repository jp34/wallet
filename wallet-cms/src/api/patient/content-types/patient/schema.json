{
  "kind": "collectionType",
  "collectionName": "patients",
  "info": {
    "singularName": "patient",
    "pluralName": "patients",
    "displayName": "Patient",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "first_name": {
      "type": "string",
      "required": true
    },
    "middle_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "birthday": {
      "type": "date",
      "required": true
    },
    "diagnoses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::diagnosis.diagnosis",
      "inversedBy": "patients"
    },
    "username": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "patient_id"
    },
    "medical_encounters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::medical-encounter.medical-encounter",
      "mappedBy": "patient"
    },
    "patient_allergies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::patient-allergy.patient-allergy",
      "mappedBy": "patient"
    },
    "patient_medications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::patient-medication.patient-medication",
      "mappedBy": "patient"
    }
  }
}
